{
  "name": "@bgd-labs/toolbox",
  "version": "0.2.13",
  "main": "./dist/index.mjs",
  "browser": "./dist/browser.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "sideEffects": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bgd-labs/toolkit.git",
    "directory": "packages/toolbox"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "test": "vitest",
    "generate": "npm run generate:explorers && npm run generate:rpcs && npm run generate:abis && npm run generate:chainlink && npm run generate:snapshot-org",
    "generate:explorers": "bun src/scripts/explorer.ts",
    "generate:rpcs": "bun src/scripts/rpc.ts",
    "generate:abis": "bun src/scripts/abi.ts",
    "generate:chainlink": "bun src/scripts/chainlink.ts",
    "generate:snapshot-org": "bunx genql --endpoint https://hub.snapshot.org/graphql --output ./src/ecosystem/generated/snapshot-org"
  },
  "devDependencies": {
    "@aave-dao/aave-umbrella": "1.0.0",
    "@aave-dao/aave-v3-origin": "3.6.0",
    "@bgd-labs/aave-address-book": "^4.31.0",
    "@bgd-labs/solidity-utils": "^1.1.2",
    "@genql/cli": "^6.3.3",
    "@ipld/dag-pb": "^4.1.3",
    "@openzeppelin/contracts": "^5.2.0",
    "@openzeppelin/contracts-upgradeable": "^5.4.0",
    "@pollyjs/adapter-fetch": "^6.0.7",
    "@pollyjs/adapter-node-http": "^6.0.6",
    "@pollyjs/core": "^6.0.6",
    "@pollyjs/persister-fs": "^6.0.6",
    "@safe-global/safe-deployments": "^1.37.49",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "catalog:",
    "@bgd-labs/aave-price-feeds": "github:bgd-labs/aave-price-feeds",
    "diff": "^8.0.2",
    "eventsource": "^3.0.6",
    "ipfs-unixfs": "^11.1.3",
    "js-yaml": "^4.1.0",
    "multiformats": "^13.4.2",
    "ts-pattern": "^5.8.0",
    "tsdown": "^0.20.3",
    "typescript": "catalog:",
    "vitest": "^3.0.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "prettier": "^3.5.2",
    "prettier-plugin-solidity": "^1.0.0",
    "viem": "catalog:"
  },
  "exports": {
    ".": "./dist/index.mjs",
    "./browser": "./dist/browser.mjs",
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=22"
  }
}
